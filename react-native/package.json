{
  "name": "react-native-dictation",
  "version": "0.1.0",
  "description": "Low-latency, native-backed dictation for React Native with real-time waveform streaming",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ios": "cd ios && xcodebuild test -scheme ReactNativeDictation -destination 'platform=iOS Simulator,name=iPhone 15'",
    "test:android": "cd android && ./gradlew test",
    "e2e:ios": "detox test --configuration ios.sim.debug",
    "e2e:android": "detox test --configuration android.emu.debug"
  },
  "keywords": [
    "react-native",
    "dictation",
    "speech-recognition",
    "waveform",
    "audio"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "dependencies": {
    "react-native-svg": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-syntax-typescript": "^7.27.1",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^19.0.0",
    "@types/react-native": "^0.72.0",
    "babel-jest": "^30.2.0",
    "detox": "^20.0.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react": "^19.2.0",
    "react-native": "^0.83.0",
    "react-test-renderer": "^19.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-native-community)/)"
    ],
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ]
  }
}
